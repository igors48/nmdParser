<processor>

	<extractTag out="tag">
		<pattern><![CDATA[<td class="thead"]]></pattern> 
		<occurrence>0</occurrence>
	</extractTag>
	
	<getGroup in="tag" out="tag">
		<pattern><![CDATA[</a>(.+?)<]]></pattern> 
		<occurrence>0</occurrence>
	</getGroup>
	
	<getGroup in="tag" >
		<pattern><![CDATA[^\s*(.+?)\s*$]]></pattern> 
		<occurrence>0</occurrence>
	</getGroup>
	
</processor>