<?xml version="1.0" encoding="cp1251"?>

<processor>

	<xPath out="pageNav">
		<expression>/body/table[2]/tbody/tr/td[@class='tableMAIN']/table[3]/tbody/tr/td/table[3]/tbody/tr/td[2]/div[@class='pagenav']/table[@class='tborder']/tbody/tr/td[@class='vbmenu_control']</expression>
	</xPath>
	
	<getGroup in="pageNav" out="maxPage">
		<pattern><![CDATA[Page\s*\d+\s*of\s*(\d+)]]></pattern>
		<occurrence>0</occurrence>
	</getGroup>
	
	<set value="&amp;page=*" out="postfix"/>

	<concat first="url" second="postfix" out="topicUrl"/>

	<set value="1" out="start" />
	<set value="1" out="step" />
	<set value="1" out="mult" />
	
	<sequence pattern="topicUrl" start="start" stop="maxPage" step="step" mult="mult"/>

</processor>