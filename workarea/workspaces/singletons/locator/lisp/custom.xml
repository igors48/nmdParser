<processor>

	<load out="titlePage">
		<url>http://lisper.ru/pcl/</url>
	</load>
	
	<xPath in="titlePage" out="links">
		<expression>/html/body/div[@id='content']/ol</expression>
	</xPath>
	
	<getGroup in="links" >
		<pattern><![CDATA[<a href="(/pcl/.+?)"]]></pattern>
		<occurrence>-1</occurrence>
	</getGroup>
	
</processor>