<processor>

	<load out="page">
		<url>http://static.springsource.org/spring/docs/2.5.x/reference/index.html</url>
	</load>
	
	<xPath in="page" out="links">
		<expression>//div[@class='toc']</expression>
	</xPath>
	
    <getGroup in="links">
        <pattern><![CDATA[href="(.[^#]+?\.html)"]]></pattern> 
        <occurrence>-1</occurrence>
    </getGroup>
	
</processor>
