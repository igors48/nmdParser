<processor>

	<load out="temporary">
		<url>http://rusrep.ru/</url>
	</load>

	<set value="http://rusrep.ru/" out="host"/>

	<load out="title_page_content" referer="host">
		<url>http://rusrep.ru/${year|2011}/${issue|08}/</url>
	</load>
	
	<xPath in="title_page_content">
		<expression>//ul/li/a[@class='sub_title']/@href</expression>
	</xPath>

</processor>