<processor>

	<load in="url" out="page_content"/>
	
	<xPath in="page_content" out="print_link_relative">
		<expression>//a[@class='print']/@href</expression>
	</xPath>
	
	<set value="http://rusrep.ru" out="host"/>
	
	<concat first="host" second="print_link_relative" out="print_link"/>
	
	<load in="print_link" referer="url" out="print_page_content"/>
	
	<xPath in="print_page_content">
		<expression>/html/body</expression>
	</xPath>
	
</processor>