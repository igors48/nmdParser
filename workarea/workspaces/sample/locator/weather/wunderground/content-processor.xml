<processor>

	<xPath out="logo" mode="tag">
		<expression>//body/table[@class='full']/tbody/tr/td[@id='left']/div[@id='logo']/a/img</expression>
	</xPath>
	
	<xPath out="content">
		<expression>//body/table[@class='full']/tbody/tr/td[@id='right']/div[@class='innerFrame']/div[@class='innerContent']/table[@class='full']/tbody/tr/td[@class='vaT full']/table[@class='full']/tbody/tr/td[@class='full vaT']/div[@class='tm10 bm15']</expression>
	</xPath>
	
	<removeTag in="content" out="content">
		<pattern><![CDATA[<script]]></pattern> 
		<occurrence>-1</occurrence>
	</removeTag>
	
	<concat first="logo" second="content"/>
	
</processor>