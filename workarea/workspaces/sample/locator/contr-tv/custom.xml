<processor>

	<load out="page">
		<url>http://www.contrtv.ru/</url>
	</load>
	
	<xPath in="page" out="links">
		<expression>/html/body/table[2]/tbody/tr/td[1]/div/table/tbody</expression>
	</xPath>
	
	<getGroup in="links">
		<pattern><![CDATA[<a href="(/common/\d+/)"]]></pattern>
		<occurrence>-1</occurrence>
	</getGroup>

</processor>