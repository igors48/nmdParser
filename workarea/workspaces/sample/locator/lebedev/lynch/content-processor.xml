<processor>

	<set value="&lt;br/&gt;" out="br"/>
	
	<xPath out="nick" >
		<expression>//body/table[2]/tbody/tr[1]/td[2]</expression>
	</xPath>
	
	<concat first="nick" second="br" out="nick"/>
	
	<xPath out="comments" >
		<expression>//body/table[2]/tbody/tr[2]/td[2]</expression>
	</xPath>
	
	<concat first="comments" second="br" out="comments"/>
	
	<xPath out="recenser" >
		<expression>//body/table[2]/tbody/tr[1]/td[3]</expression>
	</xPath>

	<concat first="recenser" second="br" out="recenser"/>
		
	<xPath out="content">
		<!--<expression>//body/div[5]</expression>-->
		<expression>//center/div[@id='Lynch']/img</expression>
	</xPath>
	
	<xPath out="recense">
		<expression>//div[@id='LynchComments']/*</expression>
	</xPath>
	
	<concat first="nick" second="comments" out="header"/>
	<concat first="header" second="recenser" out="header"/>
	<concat first="header" second="content" out="header"/>
	<concat first="header" second="recense" />
	
</processor>